{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "rootDir": "./src",
    "outDir": "build",
    "typeRoots": ["./typings", "./node_modules/@types"],
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "pretty": true,
    "skipLibCheck": true
  },
  "include": [
    "src/**/*.ts",
    "test/**/*.ts",
    "typings/**/*.ts"
  ],
"exclude": [
  "node_modules"
]
}
